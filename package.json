{
	"name": "pyluacheck",
	"displayName": "PyLuaCheck",
	"description": "检查重写父类函数,没有调用父类函数问题",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:lua"
	],
	"icon": "images/pyluacheck_icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "pyluacheck",
			"properties": {
				"pyluacheck.checkClass": {
					"type": "array",
					"default": [
						{
							"name":"Base",
							"overrideFunc":[
								"Ctor","Destroy"
							]
						},
						{
							"name":"UI.BaseUI",
							"overrideFunc":[
								"Ctor","Close"
							]
						},
						{
							"name": "Entity.EntityLogic.BaseEntity",
							"overrideFunc": [
								"Ctor",
								"OnShow",
								"OnHide"
							]
						}
					],
					"superClass": {
						"name": "string",
						"overrideFunc": "array"
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/luaparse": "^0.2.6",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"luaparse": "^0.3.0"
	},
	"publisher": "firleaves",
	"repository": {
        "url": "https://github.com/firleaves/PyLuaCheck.git"
    }
}
